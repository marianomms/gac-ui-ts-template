{
  "name": "gac-ui-ts",
  "version": "0.0.1",
  "description": "Using webpack with typescript for gac ui",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "npm run type-check && webpack --config=webpack.config.js --env.development=true",
    "build-prod": "npm run type-check && webpack --config=webpack.config.js --env.production=true",
    "lint": "eslint src --ext js --ext jsx --ext ts --ext tsx",
    "start": "npm run type-check && webpack-dev-server --config=webpack.config.js --env.development=true  --disable-host-check",
    "test": "jest --coverage",
    "type-check": "tsc --noEmit",
    "watch": "TZ=UTC node --inspect=0.0.0.0:9229 ./node_modules/.bin/jest --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
